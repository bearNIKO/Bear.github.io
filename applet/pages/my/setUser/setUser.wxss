/* pages/my/setUser/setUser.wxss */
.container {
  width: 100%;
  height: 100vh;
  background: #F5F5F5;
}
.container .head {
  width: 100%;
  height: 100rpx;
  background: #fff;
  color: #999;
  font-size: 40rpx;
  line-height: 90rpx;
  text-align: center;
}

.container .nav {
  width: 100%;
}
.container .nav .userInfo {
  position: relative;
  padding-right: 40rpx;
  height: 80rpx;
  padding-left: 40rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
}
.container .nav .userInfo .text {
  float: right;
  font-size: 24rpx;
  margin-right: 30rpx;
}
.container .nav .userInfo:active {
  background: rgba(0, 0, 0, 0.05);
}
.container .nav .userInfo .icon {
  float: right;
  font-size: 40rpx;
}
.container .nav .security {
  position: relative;
  padding-right: 40rpx;
  height: 80rpx;
  padding-left: 40rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
}
.container .nav .security:active {
  background: rgba(0, 0, 0, 0.05);
}
.container .nav .security .text {
  float: right;
  font-size: 24rpx;
  margin-right: 30rpx;
}
.container .nav .security .icon {
  float: right;
  font-size: 40rpx;
}

.container .feedBack {
  position: relative;
  padding-right: 40rpx;
  height: 80rpx;
  margin-top: 20rpx;
  padding-left: 40rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
}
.container .feedBack:active {
  background: rgba(0, 0, 0, 0.05);
}
.container .feedBack .icon {
  float: right;
  font-size: 40rpx;
}

.container .clearUsers {
  width: 100%;
  height: 100rpx;
  margin-top: 20rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 36rpx;
  color: red;
  background: #fff;
}
.container .clearUsers:active {
  background: rgba(0, 0, 0, 0.05);
}