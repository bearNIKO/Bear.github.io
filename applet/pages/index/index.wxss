/* pages/index/index.wxss */
.container {
  position: fixed;
  width: 100%;
  height: 100vh;
  background: rgb(243, 77, 186);
}

.container .head {
  position: relative;
  width: 100%;
  z-index: 110;
  background: rgb(243, 77, 186);
}
.container .head .head_search {
  display: flex;
  height: 90rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  align-items: center;
}
.container .head .head_search .search_box {
  flex: 1;
  height: 64rpx;
  padding-left: 20rpx;
  border-radius: 40rpx;
  background: #fff;
  color: #999;
  line-height: 64rpx;
  font-size: 24rpx;
}
.container .head .head_search .search_box .icon {
  display: inline-block;
  font-size: 40rpx;
  font-weight: 500;
  vertical-align: middle;
  margin-right: 10rpx;
}
.container .head .head_search .img {
  width: 64rpx;
  height: 64rpx;
  margin-left: 20rpx;
}
.container .head .head_search .message {
  font-size: 24rpx;
  font-weight: 400;
  margin-left: 24rpx;
  color: #fff;
  text-align: center;
}
.container .head .head_search .message .icon {
  font-size: 40rpx;
}


.container .head .head_nav {
  display: flex;
  align-items: center;
  height: 60rpx;
}
.container .head .head_nav .ant {

  display: inline-block;
  width: 100rpx;
  height: 60rpx;
  text-align: center;
  background: rgb(243, 77, 186);
}
.container .head .head_nav .ant text {
  display: inline-block;
  height: 50rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  color: rgba(255,255,255,.65);
}
.container .head .head_nav .ant text.active {
  border-bottom: 4rpx solid #fff;
  color: #fff;
}

.container .head .head_nav .nav_wrapper {
  flex: 1;
  overflow: hidden;
}
.container .head .head_nav .nav_wrapper .nav_wrapper_box {
  white-space: nowrap;
}
.container .head .head_nav .nav_wrapper .nav_wrapper_box .nav_wrapper_item {
  display: inline-block;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 10rpx;
  color: rgba(255,255,255,.65);
}
.container .head .head_nav .nav_wrapper .nav_wrapper_box .nav_wrapper_item.active {
  border-bottom: 4rpx solid #fff;
  box-sizing: border-box;
  color: #fff;
}

.container .head .head_nav .icon {
  font-size: 44rpx;
  line-height: 60rpx;
  width: 60rpx;
  height: 60rpx;
  color: #fff;
  background: rgb(243, 77, 186);
}



.container .banner {
  position: absolute;
  width: 100%;
  top: 150rpx;
  bottom: 0;
}

.container .banner .swiper {
  padding: 20rpx;
  height: 280rpx;
  margin-bottom: 10rpx;
}
.container .banner .swiper .muban {
  border-radius: 10rpx;
  height: 300rpx;
  background: #F5F5F5;
}
.container .banner .swiper .swiper_item {
  border-radius: 10rpx;
}
.container .banner .swiper .swiper_item .img {
  position: fixed;
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

.container .banner .main_module_layout {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.container .banner .main_module_layout .max_activity {
  height: 190rpx;
}
.container .banner .main_module_layout .max_activity .img {
  width: 100%;
  height: 100%;
}
.container .banner .main_module_layout .activity_min_group {
  margin-top: 6rpx;
  height: 140rpx;
  display: flex;
}
.container .banner .main_module_layout .activity_min_group .activity_min {
  flex: 1;
  height: 140rpx;
}
.container .banner .main_module_layout .activity_min_group .activity_min .img {
  width: 100%;
  height: 100%;
}
.container .banner .main_module_img {
  padding: 0 20rpx;
}
.container .banner .main_module_img .nav {
  padding-top: 10rpx;
  border-radius: 10rpx 10rpx 0 0;
  background: #fff;
}
.container .banner .main_module_img .nav .items .item {
  display: inline-block;
  width: 25%;
  margin-bottom: 10rpx;
}
.container .banner .main_module_img .nav .items .item .img {
  display:block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}
.container .banner .main_module_img .nav .items .item .text {
  font-size: 24rpx;
  color: #333;
  text-align: center;
}

.container .banner .main_module_img .nav .muban {
  padding-top: 10rpx;
  border-radius: 10rpx 10rpx 0 0;
  background: #fff;
}
.container .banner .main_module_img .nav .muban .item {
  display: inline-block;
  width: 25%;
  margin-bottom: 10rpx;
}
.container .banner .main_module_img .nav .muban .item .img {
  display:block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-bottom: 10rpx;
  border-radius: 50%;
  background: #EEE;
}
.container .banner .main_module_img .nav .muban .item .text {
  width: 100rpx;
  height: 20rpx;
  background: #EEE;
  margin: 0 auto;
}

.container .banner .hr {
  margin: 20rpx;
  height: 35rpx;
  background: #fff;
  border-radius: 15rpx;
}

.container .banner .table_find {
  display: flex;
  padding-bottom: 20rpx;
  align-items: center;
}
.container .banner .table_find .table_find_item{
  flex: 1;
}
.container .banner .table_find .table_find_item .title{
  text-align: center;
  font-size: 36rpx;
  color: #fff;
}
.container .banner .table_find .table_find_item .text {
  text-align: center;
  font-size: 24rpx;
  color: #fff;
}
.container .banner .table_find .table_find_item .text.active {
  width: 130rpx;
  height: 32rpx;
  border-radius: 40rpx;
  font-size: 24rpx;
  color: #FE3738;
  background: #fff;
  margin: 0 auto;
}
.container .banner .table_find .table_find_shu {
  height: 60rpx;
  width: 4rpx;
  background: #c3c3c3;
}

.container .table_find_fixed {
  position: fixed;
  top: -100rpx;
  z-index: 100;
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  background: #fff;
  transition: all 1s ease;
  z-index: 100;
}
.container .table_find_fixed .table_find_item{
  flex: 1;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
}
.container .table_find_fixed .table_find_item .title{
  display: inline;
  font-size: 36rpx;
  color: #666;
}
.container .table_find_fixed .table_find_item .title.active {
  color: #fe3a33;
  padding-bottom: 14rpx;
  border-bottom: 6rpx solid #fe3a33;
}
.container .table_find_fixed .table_find_shu {
  height: 60rpx;
  width: 4rpx;
  background: #fff;
}
.container .table_find_fixed.active {
  top: 150rpx;
}



.container .banner .goodsList {
  width: 100%;
  background: rgb(243, 77, 186);
}
.container .banner .goodsList .goodsItems {
  width: 50%;
  box-sizing: border-box;
  display: inline-block;
  margin-bottom: 20rpx;
}
.container .banner .goodsList .goodsItems:nth-child(even) {
  padding-left: 10rpx;
  padding-right: 20rpx;
} 
.container .banner .goodsList .goodsItems:nth-child(odd) {
  padding-left: 20rpx;
  padding-right: 10rpx;
}
.container .banner .goodsList .goodsItems .cent {
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.container .banner .goodsList .goodsItems .cent .img {
  width: 100%;
  height: 360rpx;
}
.container .banner .goodsList .goodsItems .cent .mar {
  position: relative;
  z-index: 1;
  zoom: 1;
  padding: 30rpx 18rpx 18rpx 14rpx;
  box-sizing: border-box;
  height: 222rpx;
  margin: 0!important;
  display: flex;
  flex-direction: column;
}
.container .banner .goodsList .goodsItems .cent .mar .title {
  font-size: 26rpx;
  font-weight: 400;
  color: #333;
  height: 32rpx;
  padding-right: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.container .banner .goodsList .goodsItems .cent .mar .title .icon {
  display: inline-block;
  width: 48rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info {
  color: #333;
  position: absolute; 
  bottom: 20rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .price {
  font-size: 24rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: 400;
  color: #666;
  margin-bottom: -2rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .price .ju {
  height: 24rpx;
  width: 24rpx;
  margin-top: -5rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .price .red_bubble {
  position: relative;
    z-index: 1;
    zoom: 1;
    font-size: 22rpx;
    font-weight: 400;
    color: #fff;
    background: linear-gradient(150deg,#fe3a33,#fb8d2c);
    line-height: 24rpx;
    margin-left: 10rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
    height: 24rpx;
    top: -2px;
    display: inline-block;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .price .RMB {
  display: inline-block;
  font-size: 26rpx;
  font-weight: 500;
  color: #FF2B22;
  margin-left: 4rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .price .price_num {
  font-size: 35rpx;
  font-weight: 500;
  color: #FF2B22;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .label_box {
  margin-right: 4rpx;
  font-size: 28rpx;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .label_box .juan {
  height: 28rpx;
  background: linear-gradient(90deg,#ff8873 0,#ff4f4f 100%);
  border-radius: 4rpx;
  padding: 0 10rpx 0 2rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #fff;
  line-height: 28rpx;
  display: inline-block;
  vertical-align: middle;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .label_box .icon {
    height: 28rpx;
    border-radius: 4rpx;
    border: 2rpx solid;
    font-size: 22rpx;
    font-weight: 400;
    color: #ff0137;
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin-right: 10rpx;
    display: inline-block;
    line-height: 28rpx;
    vertical-align: middle;
    border-color: rgba(245,114,36,.7);
    color: #f57223;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .label_box .juan .text {
    background: #fff;
    border-radius: 2rpx 0 0 2rpx;
    font-size: 20rpx;
    margin-top: 1rpx;
    color: #FE3A33;
    width: 24rpx;
    height: 24rpx;
    display: inline-block;
    line-height: 26rpx;
    text-align: center;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .salse {
  font-size: 22rpx;
  font-weight: 400;
  color: #888;
}
.container .banner .goodsList .goodsItems .cent .mar .product_info .salse .shu{
  content: '';
  width: 2rpx;
  background: #888;
  margin: 0 10rpx;
  display: inline-block;
  height: 16rpx;
  position: relative;
  z-index: 1;
  zoom: 1;
  top: 2rpx;
}
.container .banner .print_list_recommend {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  background: #EEE;
}
.container .banner .print_list_recommend .text {
  position: relative;
  text-align: center;
  height: 40rpx;
  color: #FC3F78;
  font-size: 30rpx;
}
.container .banner .print_list_recommend .text .icon{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  margin-right: 10rpx;
  background: #FC3F78;
  text-align: center;
  font-size: 26rpx;
  border-radius: 50%;
}
.container .banner .print_list_recommend .text .henl {
  position: relative;
  display: inline-block;
  width: 150rpx;
  vertical-align: middle;
  height: 2rpx;
  text-align: right;
  border-top: solid 2rpx #FC3F78;
  margin-right: 15rpx;
}
.container .banner .print_list_recommend .text .henr {
  position: relative;
  display: inline-block;
  width: 150rpx;
  vertical-align: middle;
  margin-left: 15rpx;
  height: 2rpx;
  text-align: right;
  border-top: solid 2rpx #FC3F78;
}
.container .banner .print_list_recommend .text .henl .qiu {
  position: absolute;
  top: -4rpx;
  right: 0;
  display: inline-block;
  width: 6rpx;
  height: 6rpx;
  content: "";
  border-radius: 50%;
  overflow: hidden;
  background: #FC3F78;
}
.container .banner .print_list_recommend .text .henr .qiu {
  position: absolute;
  top: -4rpx;
  left: 0;
  display: inline-block;
  width: 6rpx;
  height: 6rpx;
  content: "";
  border-radius: 50%;
  overflow: hidden;
  background: #FC3F78;
}




.container .back_top {
  position: fixed;
  z-index: 100;
  bottom: -100rpx;
  right: 40rpx;
  width: 70rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 40rpx;
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  border-radius: 50%;
  transition: all .5s ease;
}
.container .back_top.back_top_show {
  bottom: 100rpx;
}


::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}